{
  "name": "@envenv/accounts-service",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "gen-graphql": "graphql-codegen --config codegen.yml",
    "gen-graphql-watch": "graphql-codegen --config codegen.yml --watch src/graphql/schemas/*.graphql",
    "sync-db-migrations": "prisma migrate up --experimental && prisma generate",
    "dev": "concurrently \"ts-node-dev --respawn --transpileOnly --poll --interval 1000 --watch src/graphql/generated.ts src/index.ts\" \"npm run gen-graphql-watch\" \"npm run sync-db-migrations\"",
    "apollo-push:dev": "apollo service:push --serviceURL=http://dev-accounts-service:5000/graphql --serviceName=Accounts --variant=development",
    "test": "jest"
  },
  "keywords": [],
