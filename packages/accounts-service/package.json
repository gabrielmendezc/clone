    "apollo-server-express": "^2.13.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "cryptr": "^6.0.2",
    "date-fns": "^2.14.0",
    "express": "^4.17.1",
    "generate-password": "^1.5.1",
    "graphql": "^15.0.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.3.0",
    "graphql-tag": "^2.10.3",
