    "@graphql-codegen/typescript-resolvers": "1.14.0",
    "@prisma/cli": "^2.0.0-beta.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cryptr": "^4.0.1",
