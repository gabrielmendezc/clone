datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id                 Int    @default(autoincrement()) @id
  picture            String
  provider           String
  username           String @unique
  email              String @unique
  name               String
  password           String
  role               String
  lastPasswordChange String
}