    "cookie-parser": "^1.4.5",
    "graphql": "^15.0.0",
    "graphql-import": "^1.0.2",
    "graphql-request": "^1.8.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.13.5",
    "@graphql-codegen/typescript": "1.13.5",
    "@graphql-codegen/typescript-resolvers": "1.13.5",
    "@prisma/cli": "^2.0.0-beta.5",
    "@types/cookie-parser": "^1.4.2",
    "@types/node": "^14.0.1",
