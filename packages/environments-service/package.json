{
  "name": "environments-service",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "gen-graphql": "graphql-codegen --config codegen.yml",
    "gen-graphql-watch": "graphql-codegen --config codegen.yml --watch src/graphql/schema.graphql",
    "dev": "concurrently \"ts-node-dev --respawn --transpileOnly --poll --interval 1000 --watch src/graphql/schema.graphql src/index.ts\" \"npm run gen-graphql-watch\""
  },
